<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
    >

    <TextView
        style="@style/TitleStyle"
        android:text="设置中心"
        />

    <cn.itcast.phonesafe.view.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/cn.itcast.phonesafe"
        android:id="@+id/siv_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:destitle="自动更新设置"
        mobilesafe:desoff="自动更新已关闭"
        mobilesafe:deson="自动更新已开启">
    </cn.itcast.phonesafe.view.SettingItemView>
    <cn.itcast.phonesafe.view.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/cn.itcast.phonesafe"
        android:id="@+id/siv_phoneShow"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:destitle="电话归属地的显示设置"
        mobilesafe:desoff="归属地显示已关闭"
        mobilesafe:deson="归属地显示已开启">
    </cn.itcast.phonesafe.view.SettingItemView>

    <cn.itcast.phonesafe.view.SettingClickView
        android:id="@+id/scv_toast_tyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
       >
    </cn.itcast.phonesafe.view.SettingClickView>
    <cn.itcast.phonesafe.view.SettingClickView
        android:id="@+id/scv_toast_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
    </cn.itcast.phonesafe.view.SettingClickView>

    <cn.itcast.phonesafe.view.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/cn.itcast.phonesafe"
        android:id="@+id/siv_blank_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:destitle="黑名单拦截设置"
        mobilesafe:desoff="黑名单拦截已关闭"
        mobilesafe:deson="黑名单拦截已开启">
    </cn.itcast.phonesafe.view.SettingItemView>
</LinearLayout>
